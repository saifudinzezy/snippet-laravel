	"Ajax Result": {
        "scope": "javascript",
        "prefix": "fe-ajax_result",
        "body": [
            "\\$.ajax({",
            "\turl: scope.url + '/result',",
            "\ttype: 'GET',",
            "\tbeforeSend: function () {",
            "\t\t\\$(\".loader\").fadeIn();",
            "\t}, ",
            "\tsuccess: function (a) {",
            "\t\t\\$.extend(scope, \\$.parseJSON(a));",
            "\t\t// your code",
            "\t}, error: function (er) {",
            "\t\tconsole.log(er.responseText);",
            "\t\t\\$(\".loader\").fadeOut('slow');",
            "\t}",
            "});"
        ],
        "description": "Ajax Result "
    },
